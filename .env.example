# Solana RPC Configuration
RPC_ENDPOINT=https://api.mainnet-beta.solana.com
WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com

# For development/testing, you can use devnet:
# RPC_ENDPOINT=https://api.devnet.solana.com
# WEBSOCKET_ENDPOINT=wss://api.devnet.solana.com

# Private key for transactions (base58 encoded)
# WARNING: Never commit your actual private key to version control
PRIVATE_KEY=your_base58_encoded_private_key_here

# Default commitment level
COMMITMENT=confirmed

# Default slippage tolerance (in basis points)
DEFAULT_SLIPPAGE=500

# Priority fees (optional)
PRIORITY_FEE_UNIT_LIMIT=200000
PRIORITY_FEE_UNIT_PRICE=1000

# Logging level
LOG_LEVEL=INFO

